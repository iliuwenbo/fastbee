<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fastbee.platform.mapper.ApiRequestExampleMapper">

    <resultMap type="com.fastbee.platform.domain.ApiRequestExample" id="ApiRequestExampleResult">
        <result property="id"    column="id"    />
        <result property="apiId"    column="api_id"    />
        <result property="requestExample"    column="request_example"    />
        <result property="requestHeadersExample"    column="request_headers_example"    />
        <result property="delFlag"    column="del_flag"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectApiRequestExampleVo">
        select id, api_id, request_example, request_headers_example, del_flag, create_by, create_time, update_by, update_time, remark from api_request_example
    </sql>

</mapper>
